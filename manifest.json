{
  "manifest_version": 2,

  "name": "Animated Emojis",
  "description": "This extension makes some of your WhatsApp Web emojis animated.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "css": ["animations.css"]
    }
  ],
  "browser_action": {
    "default_title": "Rolling!",
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ]
}
